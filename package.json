	"packageManager": "pnpm@9.4.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"test:e2e": "playwright test && playwright test --browser=webkit",
		"test:ui": "playwright test --ui",
		 "test-and-push": "echo 'Tailscale connectâ€¦' && tailscale up --exit-node=100.86.31.118 --accept-routes --exit-node-allow-lan-access && echo 'Running tests...' && vitest --run && playwright test --browser=chromium && playwright test --browser=webkit && echo 'Pushing to git...' && git push origin master && echo 'Resetting Tailscale configuration...' && tailscale up --reset && echo 'Tailscale connected without exit node.' && tailscale up"
	},
	"devDependencies": {
		"@eslint-community/eslint-utils": "^4.7.0",
