    "eslint-plugin-svelte": "^2.46.0",
    "playwright": "^1.48.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^5.1.2",
    "svelte-check": "^3.8.6",
