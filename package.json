		"playwright": "^1.54.2",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.0",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.11",
